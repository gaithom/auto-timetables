{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "accent-color",
          "text-size-adjust",
          "-webkit-overflow-scrolling",
          "scroll-behavior",
          "scroll-behavior: smooth"
        ]
      }
    ]
  }
}